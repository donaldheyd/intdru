; Popovers
; --------------
settings[bootstrap_popover_enabled] = 1
settings[bootstrap_popover_animation] = 1
settings[bootstrap_popover_html] = 0
settings[bootstrap_popover_placement] = 'right'
settings[bootstrap_popover_selector] = ''
